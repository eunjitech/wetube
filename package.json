{
  "name": "wetube",
  "version": "1.0.0",
  "description": "Cloning Youtube with Vanilla JS and NodeJS",
  "main": "app.js",
  "scripts": {
    "dev:server": "nodemon --exec babel-node ./src/init.js --delay 2 --ignore '.scss' --ignore 'static'",
    "dev:assets": "cd src && EBPACK_ENV=development webpack -w",
    "build:assets": "cd src && WEBPACK_ENV=production webpack",
    "build:server": "babel src --out-dir build --ignore 'src/assets','src/static','src/webpack.config.js'",
    "copyAll": "cp -R src/static build && cp -R src/views build",
    "build": "npm run build:server && npm run build:assets && npm run copyAll",
    "prebuild": "rm -rf build",
    "ngrok": "ngrok http 4000",
    "start": "node build/init.js"
  },
  "author": "leeeunji",
  "license": "ISC",
  "dependencies": {
    "@babel/polyfill": "^7.12.1",
    "autoprefixer": "^10.2.5",
    "aws-sdk": "^2.888.0",
    "axios": "^0.21.1",
    "babel-loader": "^8.2.2",
    "connect-mongo": "^4.4.1",
    "cookie-parser": "^1.4.5",
    "core-js": "^3.9.1",
    "css-loader": "^5.2.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "helmet": "^4.4.1",
    "mongoose": "^5.12.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "multer-s3": "^2.9.0",
    "passport": "^0.4.1",
    "passport-facebook": "^3.0.0",
    "passport-github": "^1.1.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-local": "^1.0.0",
    "passport-local-mongoose": "^6.1.0",
    "postcss-loader": "^5.2.0",
    "pug": "^3.0.2",
    "sass": "^1.32.8",
    "sass-loader": "^11.0.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.10",
    "@babel/core": "^7.13.10",
    "@babel/node": "^7.13.12",
    "@babel/preset-env": "^7.13.12",
    "eslint": "^7.23.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.1",
    "mini-css-extract-plugin": "^1.4.0",
    "nodemon": "^2.0.7",
    "prettier": "^2.2.1",
    "webpack": "^5.28.0",
    "webpack-cli": "^4.6.0"
  }
}
